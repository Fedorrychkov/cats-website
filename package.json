    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "!(*{tests,use-cases}/**).js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "tests/**/*.js": [
      "tests/node_modules/.bin/eslint"
    ],
    "use-cases/**/*.js": [
      "use-cases/node_modules/.bin/eslint"
    ],
    "*.ts": [
      "prettier --parser typescript --write --no-semi",
      "bash -c tsc",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
